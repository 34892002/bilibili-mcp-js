{
  "name": "bilibili-mcp-js",
  "module": "index.ts",
  "type": "module",
  "private": false,
  "version": "0.1.3",
  "description": "Bilibili video search MCP (Model Context Protocol) service - 哔哩哔哩视频搜索MCP服务",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "bilibili-mcp": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "README-en.md", 
    "README-ja.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "tsx index.ts",
    "test": "node test.js",
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "build:bun": "bun build index.ts --outfile ./dist/index.js --target node",
    "inspector": "npx @modelcontextprotocol/inspector tsx index.ts",
    "inspector:dist": "npx @modelcontextprotocol/inspector ./dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/tough-cookie": "^4.0.5",
    "typescript": "^5.8.2",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.57",
    "@langchain/openai": "^0.4.9",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "express": "^5.1.0",
    "@types/express": "^5.0.4",
    "axios": "^1.8.4",
    "langchain": "^0.3.19",
    "tough-cookie": "^5.1.2"
  },
  "keywords": [
    "bilibili",
    "search",
    "video",
    "mcp",
    "ai",
    "model-context-protocol",
    "anime",
    "bangumi",
    "timeline",
    "hot-content"
  ],
  "author": "ggyy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/34892002/bilibili-mcp-js"
  },
  "homepage": "https://github.com/34892002/bilibili-mcp-js#readme"
}
